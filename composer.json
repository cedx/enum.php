{
	"description": "A simple implementation of enumerated types.",
	"homepage": "https://docs.belin.io/enum.php",
	"license": "MIT",
	"name": "cedx/enum",
	"type": "library",
	"version": "8.2.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"psr-4": {"Enum\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"Enum\\": "test/"}
	},
	"config": {
		"optimize-autoloader": true
	},
	"funding": [
		{"type": "patreon", "url": "https://www.patreon.com/cedx"}
	],
	"keywords": [
		"enum",
		"enumeration",
		"php",
		"type"
	],
	"require": {
		"php": ">=7.4.0",
		"ext-reflection": "*"
	},
	"require-dev": {
		"cedx/coveralls": "^13.2.0",
		"phpstan/phpstan": "^0.12.55",
		"phpunit/phpunit": "^9.4.3"
	},
	"scripts": {
		"clean": "pwsh tool/clean.ps1",
		"coverage": "coveralls var/coverage.xml",
		"doc": ["@version", "pwsh tool/doc.ps1"],
		"lint": "phpstan analyse --configuration=etc/phpstan.neon",
		"publish": ["@clean", "@version", "pwsh tool/publish.ps1"],
		"test": "phpunit --configuration=etc/phpunit.xml",
		"version": "pwsh tool/version.ps1"
	},
	"support": {
		"docs": "https://api.belin.io/enum.php",
		"issues": "https://git.belin.io/cedx/enum.php/issues"
	}
}
